{
  "name": "sagas-test",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "formik": "^2.1.3",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.0",
    "typeface-roboto": "^0.0.75",
    "yup": "^0.28.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:staged": "lint-staged",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "prettify": "prettier --write",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint './src/**/*.{js,ts}'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:ts": "npm run lint:tslint",
    "lint:tslint": "tslint --config ./tslint.json src/**/*.{ts,tsx} --project ./tsconfig.json --fix && npm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ],
    "*.ts": "npm run lint:tslint",
    "*.tsx": "npm run lint:tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.2",
    "@types/yup": "^0.26.29",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.2.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.5",
    "typescript-plugin-styled-components": "^1.4.4"
  }
}
